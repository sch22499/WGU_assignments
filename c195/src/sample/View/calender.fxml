<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller.Calender">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <center>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab text="Month View">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView prefHeight="376.0" prefWidth="600.0">
                                <columns>
                                  <TableColumn prefWidth="75.0" text="Time" />
                                  <TableColumn prefWidth="123.0" text="Appointment Type" />
                                    <TableColumn prefWidth="92.0" text="Customer" />
                                </columns>
                              </TableView>
                           </children></AnchorPane>
                  </content>
                </Tab>
                <Tab text="Week View">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView prefHeight="376.0" prefWidth="600.0">
                                 <columns>
                                    <TableColumn prefWidth="75.0" text="Time" />
                                    <TableColumn prefWidth="123.0" text="Appointment Type" />
                                    <TableColumn prefWidth="92.0" text="Customer" />
                                 </columns>
                              </TableView>
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </center>
         <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" text="Add Apointment" />
                  <Button disable="true" mnemonicParsing="false" text="Modify Appointment" />
                  <Button mnemonicParsing="false" text="Delete Appointment" />
                  <Button mnemonicParsing="false" text="Home" />
               </children>
            </HBox>
         </bottom>
         <top>
            <Label graphicTextGap="10.0" text="Schedule Panel" BorderPane.alignment="CENTER">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
